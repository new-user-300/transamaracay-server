	"scripts": {
		"build": "nest build",
		"start": "node dist/main",
		"start:dev": "cross-env NODE_ENV=development nest start --watch",
		"start:debug": "nest start --debug --watch",
		"lint": "eslint \"{source,apps,libs,test}/**/*.ts\" --fix",
		"typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./source/config/mysql.config.ts",
		"typeorm:create": "npm run typeorm migration:create",
		"typeorm:migrate": "cross-env NODE_ENV=development npm run typeorm migration:generate ./source/migrations/migrations",
		"typeorm:run": "npm run typeorm migration:run"
	},
	"dependencies": {
		"@nestjs-modules/mailer": "^1.7.1",
		"@nestjs/axios": "^0.1.0",
		"@nestjs/common": "^8.4.7",
		"@nestjs/config": "^2.1.0",
		"@nestjs/core": "^8.4.7",
		"@nestjs/jwt": "^8.0.1",
		"@nestjs/mapped-types": "1.1.0",
		"@nestjs/passport": "^8.2.2",
		"@nestjs/platform-express": "^8.4.7",
		"@nestjs/swagger": "^6.1.3",
		"@nestjs/typeorm": "^8.1.3",
		"@t00nday/nestjs-pdf": "^3.0.5",
		"axios": "^0.27.2",
		"bcrypt": "5.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "0.13.2",
		"compression": "1.7.4",
		"dotenv": "^16.0.1",
		"exceljs": "^4.3.0",
		"handlebars": "4.7.7",
		"mysql2": "^2.3.3",
		"nest-winston": "^1.7.0",
		"nodemailer": "6.7.7",
		"passport": "0.6.0",
		"passport-jwt": "4.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.5.6",
		"sharp": "0.30.7",
		"typeorm": "^0.3.7",
		"typeorm-naming-strategies": "^4.1.0",
		"uuid": "8.3.2",
		"winston": "^3.8.1"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.2.6",
		"@nestjs/schematics": "^8.0.11",
		"@types/bcrypt": "5.0.0",
		"@types/express": "4.17.13",
		"@types/multer": "1.4.7",
		"@types/node": "18.0.6",
		"@types/nodemailer": "6.4.4",
		"@types/passport-jwt": "3.0.6",
		"@types/sharp": "0.30.4",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "5.30.6",
		"@typescript-eslint/parser": "5.30.6",
		"cross-env": "^7.0.3",
		"eslint": "8.20.0",
		"ts-loader": "9.3.1",
		"ts-node": "10.9.1",
		"tsconfig-paths": "4.0.0",
		"typescript": "4.7.4"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "source",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			".+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
